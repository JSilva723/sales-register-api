generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Category {
    id       Int     @id @default(autoincrement())
    name     String  @unique @db.VarChar(20)
    isActive Boolean @default(true)
}

model Payment {
    id       Int     @id @default(autoincrement())
    name     String  @unique @db.VarChar(20)
    isActive Boolean @default(true)
}
